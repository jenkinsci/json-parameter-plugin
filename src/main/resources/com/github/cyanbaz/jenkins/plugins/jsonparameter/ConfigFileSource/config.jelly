<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="${%folder.scope.label}" field="folderScoped">
        <f:checkbox/>
    </f:entry>

    <f:entry title="${%folder.path.label}" field="folderPath" description="${%folder.path.description}">
        <f:textbox/>
    </f:entry>

    <f:entry title="${%config.file.id.label}" field="configId" description="${%config.file.id.description}">
        <f:textbox/>
    </f:entry>
</j:jelly>